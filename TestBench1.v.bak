    SW = 4'b1100; #5;
    SW = 4'b1101; #5;
    SW = 4'b1110; #5;
    SW = 4'b1111; #5;

    // KEY = 1
    KEY = 1'b1;
    SW = 4'b0000; #5;
    SW = 4'b0001; #5;
    SW = 4'b0010; #5;
    SW = 4'b0011; #5;
    SW = 4'b0100; #5;
    SW = 4'b0101; #5;
    SW = 4'b0110; #5;
    SW = 4'b0111; #5;
    SW = 4'b1000; #5;
    SW = 4'b1001; #5;
